{
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": [
    {
      "target": "www",
      "public": "build",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
