    "@types/node-fetch": "^2.5.8",
    "@types/uuid": "^8.3.0",
    "access": "^1.0.2",
    "access-control": "^1.0.1",
    "csvtojson": "^2.0.10",
    "firebase-admin": "^9.5.0",
    "firebase-functions": "^3.11.0",
    "fs": "^0.0.1-security",
    "graphql": "^16.0.1",
    "graphql-request": "^3.6.1",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "typescript": "^4.4.4",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.2.0"
